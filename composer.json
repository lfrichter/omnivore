{
    "name": "lfrichter/omnivore",
    "description": "Laravel Facade to easy access Omnivore Api",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Luis Fernando Richter",
            "email": "fernando.richter@gmail.com",
            "homepage": "https://www.linkedin.com/in/lfrichter/"
        }
    ],
    "homepage": "https://github.com/lfrichter/omnivore",
    "keywords": ["Laravel", "omnivore"],
    "require": {
        "illuminate/support": "~5",
        "sapioweb/omnivore": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "lfrichter\\omnivore\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "app/",
            "lfrichter\\omnivore\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "lfrichter\\omnivore\\omnivoreServiceProvider"
            ],
            "aliases": {
                "omnivore": "lfrichter\\omnivore\\Facades\\omnivore"
            }
        }
    }
}
